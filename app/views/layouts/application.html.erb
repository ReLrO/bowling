<!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
		<% flash.each do |key, value| %>
			<%= content_tag(:div, value, :class => "flash #{key}" ) %>
	    <% end %>

<%= yield %>

</body>
</html>
